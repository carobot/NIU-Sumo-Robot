void setup() {
  Serial.begin(9600);
  Wire.begin();

  if (accel.begin() == false) {
    Serial.println("Not Connected. Please check connections and read the hookup guide.");
    while (1);
  }
  FastLED.addLeds<NEOPIXEL, DATA_PIN>(leds, NUM_LEDS);  // GRB ordering is assumed
  FastLED.setBrightness(BRIGHTNESS);
  for(int i = 0; i<6; i++){    // turn on the leds one by one 
        leds[i] = CRGB::Red;
      }
      FastLED.show();               // sends all the led data to all the leds
      delay(100);
}

void loop() {
  if (accel.available()) {      // Wait for new data from accelerometer
    // Prints "Collision" each time accelerometer detects a tap
    if (accel.readTap() > 0) {
      Serial.println("Collision");
      startPlayback(sample, sizeof(sample));
      for(int i = 0; i<6; i++){    // turn on the leds one by one 
        leds[i] = CRGB::White;
      }
      FastLED.show();               // sends all the led data to all the leds
      delay(200);                   // To keep the leds on for a while
      for(int i = 0; i<6; i++){    // turn on the leds one by one 
        leds[i] = CRGB::Black;
      }
      FastLED.show();               // sends all the led data to all the leds
      delay(100);
      for(int i = 0; i<6; i++){    // turn on the leds one by one 
        leds[i] = CRGB::White;
      }
      FastLED.show();               // sends all the led data to all the leds
      delay(100);  
    for(int i = 0; i<6; i++){    // turn off the leds 
    leds[i] = CRGB::Red;
  }
  FastLED.show();  
    }
  }

}
